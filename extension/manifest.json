{
  "name":"mink.io",
  "version":"0.1",
  "description":"For better video watching experience on voice driven video player. Please write to feedbackfornammi.com for any bug. Big thanks to Youtube.com !!",
  "manifest_version":2,
  "icons": {
  },
  "permissions": [
    "webNavigation"
  ],
  "content_scripts":[
    {
      "matches":[
        "https://www.youtube.com/watch?v=*"
      ],
      "exclude_globs":[
      ],
      "run_at":"document_idle",
      "js":[
        "yt/cash.min.js",
        "yt/helpers.js",
        "yt/main.js"
      ],
      "css":[
        "yt/main.css"
      ]
    }
  ],
  "web_accessible_resources": [
  ]
}
